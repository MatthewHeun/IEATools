citHeader("To cite package ‘IEATools’ in publications use:")

citEntry(entry = "Manual",
  title        = "IEATools: Tools for munging and manipulating IEA extended energy balance data",
  author       = personList(as.person("Matthew Kuperus Heun"), 
                            as.person("Emmanuel Aramendia"),
                            as.person("Zeke Marshall")),
  year         = "2021",
  note         = "R package version 0.1.58",
  url          = "https://github.com/MatthewHeun/IEATools",

  textVersion  =
  paste("Matthew Kuperus Heun , Emmanuel Aramendia, Zeke Marshall (2021).",
        "IEATools: Tools for munging and manipulating IEA extended energy balance data.",
        "R package version 0.1.58.")
)
